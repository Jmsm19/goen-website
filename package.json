{
  "name": "goen_maracaibo_frontend",
  "version": "1.0.0",
  "homepage": "*",
  "main": "server.js",
  "author": "Jorge Soto <Jorgemsm19@gmail.com>",
  "license": "MIT",
  "description": "GOEN Maracaibo's website frontend",
  "adonis-version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "front": "cd frontend && yarn",
    "setup": "yarn install && yarn frontend install",
    "dev": "concurrently --kill-others --raw \"yarn dev:client\" \"yarn dev:server\" \"yarn styleguide\"",
    "dev:client": "cd frontend && yarn start",
    "dev:server": "adonis serve --dev",
    "clear:public": "rm -rf public/*",
    "move:build": "mv ./frontend/build/* ./public/ && rm -rf ./frontend/build",
    "build": "yarn clear:public && cd frontend && yarn build && cd .. && yarn move:build",
    "styleguide": "yarn front styleguide",
    "test": "nyc --reporter=lcov node ace test --bail"
  },
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/antl": "^2.0.7",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/mail": "^3.0.9",
    "@adonisjs/validator": "^5.0.6",
    "@adonisjs/vow": "^1.0.17",
    "mysql": "^2.16.0",
    "superagent": "^5.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "sqlite3": "^4.0.6"
  },
  "autoload": {
    "App": "./app"
  }
}