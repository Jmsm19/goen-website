{
  "name": "goen_maracaibo_frontend",
  "version": "1.0.0",
  "engines": {
    "node": ">=8 <12"
  },
  "main": "index.html",
  "author": "Jorge Soto <Jorgemsm19@gmail.com>",
  "license": "MIT",
  "description": "GOEN Maracaibo's website frontend",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn install --production=false && yarn react-scripts build && yarn build-sw",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-sw": "node ./src/sw-build.js",
    "clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test-cov": "yarn test -- --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@kevinwolf/formal-web": "^0.1.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "enquire.js": "^2.1.6",
    "i18next": "^15.0.9",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "js-cookie": "^2.2.0",
    "polished": "^3.0.3",
    "query-string": "^5.1.1",
    "react": "^16.8.6",
    "react-detect-offline": "^2.3.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-i18next": "^10.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.8",
    "react-toastify": "^5.1.0",
    "shards-react": "^1.0.3",
    "styled-icons": "^7.14.0",
    "uuid": "^3.3.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "jest": "^23.6.0",
    "jest-dom": "^3.1.3",
    "prettier": "^1.16.4",
    "react-pose": "^4.0.7",
    "react-styleguidist": "^9.0.4",
    "react-testing-library": "^6.1.2",
    "styled-components": "^4.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/components/**/*.{js,jsx}",
      "!**/components/App/*.{js,jsx}"
    ]
  }
}
