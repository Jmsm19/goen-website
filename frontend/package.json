    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "enquire.js": "^2.1.6",
    "i18next": "^15.0.9",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "js-cookie": "^2.2.0",
    "notistack": "^0.8.5",
    "nprogress": "^0.2.0",
    "polished": "^3.0.3",
    "query-string": "^6.8.0",
    "react": "^16.8.6",
